{
  "name": "@gameshield/behavior-analyzer",
  "version": "1.0.0",
  "description": "Behavior analysis utilities for GameShield CAPTCHA",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "audit": "npm audit",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "behavior",
    "analysis",
    "captcha",
    "gameshield",
    "security"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "typescript": "^5.0.4",
    "jest": "^29.5.0",
    "@types/jest": "^29.5.1",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "typedoc": "^0.24.7"
  }
}
