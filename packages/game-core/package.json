{
  "name": "game-core",
  "version": "1.0.0",
  "description": "Core game engine for GameShield CAPTCHA",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "docs": "typedoc --out docs src"
  },
  "keywords": [
    "game",
    "captcha",
    "security",
    "gameshield"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "pixi.js": "^8.8.1",
    "gsap": "^3.12.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^22.13.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.0",
    "typedoc": "^0.25.4"
  }
}
